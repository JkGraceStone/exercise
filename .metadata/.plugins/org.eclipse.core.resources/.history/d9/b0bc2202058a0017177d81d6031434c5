package leet.sort;

public class Sort {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//10:06
		
	}
	public static void inserSort(int[] a){
		int i,j,temp;
		int n = a.length;
		for(i =0 ;i < n-1;i++){
			temp = a[i+1];
			j= i;
			while(j>-1 && temp<=a[j]){
				a[j+1] = a[j];
				
			}
		}
	}

}
