		// int target = 9;
		// int[] result = twosum(nums, target);
		// for(int i :result){
		// System.out.println(i);
		// 375
		// Scanner s = new Scanner(System.in);
		// String name = s.nextLine();
		// int ival = s.nextInt();
		// System.out.println(ival + "," + name);
		System.out.println(getMoneyAmount(10));

	}

}
