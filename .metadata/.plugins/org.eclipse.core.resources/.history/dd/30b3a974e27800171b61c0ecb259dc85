	}

	public static int reverse(int x) {
		int result = 0;
		if (Math.abs(x) < 10) {
			return x;
		}
		while (Math.abs(x) != 0) {
			int tail = x % 10;
			int resultNew = result * 10 + tail;
			if ((resultNew - tail) / 10 != result) {
				return 0;
			}
			result = resultNew;
			x /= 10;
		}
		return result;
	}

}
